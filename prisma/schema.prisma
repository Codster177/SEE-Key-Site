datasource db {
  provider = "sqlite"
}

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

model Entry {
  id          Int      @id @default(autoincrement())
  publicKey   String   @unique
  teamName    String
  ipIncrement Int
  createdAt   DateTime @default(now())
}

model Admin {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Config {
  key   String @id
  value String
}
